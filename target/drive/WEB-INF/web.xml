<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>




	<servlet>
		<servlet-name>Signup</servlet-name>
		<display-name>Signup</display-name>
		<description></description>
		<servlet-class>servers.Signup</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>servers.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SaveFile</servlet-name>
		<display-name>SaveFile</display-name>
		<description></description>
		<servlet-class>servers.SaveFile</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FileDownload</servlet-name>
		<display-name>FileDownload</display-name>
		<description></description>
		<servlet-class>servers.FileDownload</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteFile</servlet-name>
		<display-name>DeleteFile</display-name>
		<description></description>
		<servlet-class>servers.DeleteFile</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description></description>
		<servlet-class>servers.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TestOne</servlet-name>
		<display-name>TestOne</display-name>
		<description></description>
		<servlet-class>servers.TestOne</servlet-class>
	</servlet>



	<servlet-mapping>
		<servlet-name>Signup</servlet-name>
		<url-pattern>/Signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SaveFile</servlet-name>
		<url-pattern>/SaveFile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FileDownload</servlet-name>
		<url-pattern>/FileDownload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteFile</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TestOne</servlet-name>
		<url-pattern>/TestOne</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>30</session-timeout> <!-- Timeout in minutes -->
	</session-config>




</web-app>
